<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  	<display-name>Archetype Created Web Application</display-name>
  	
   	<servlet>
    	<servlet-name>ResearchTeachingServlet</servlet-name>
    	<servlet-class>teachingPackage.ResearchTeachingServlet</servlet-class>
	</servlet>
	
	<servlet>
    	<servlet-name>ListAndResearchServlet</servlet-name>
    	<servlet-class>teachingPackage.ListAndResearchServlet</servlet-class>
	</servlet>
	  	<servlet>
    	<servlet-name>TeachingServlet</servlet-name>
    	<servlet-class>teachingPackage.TeachingServlet</servlet-class>
	</servlet>
	
	<servlet>
  		<servlet-name>ShowChargeTeacherServlet</servlet-name>
  		<servlet-class>teacherPackage.ShowChargeTeacherServlet</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
    	<servlet-name>ResearchTeachingServlet</servlet-name>
    	<url-pattern>/searchTeaching</url-pattern>
	</servlet-mapping>   
	<servlet-mapping>
    	<servlet-name>ListAndResearchServlet</servlet-name>
    	<url-pattern>/listandsearch</url-pattern>
	</servlet-mapping>
  	
  	<servlet-mapping>
    	<servlet-name>TeachingServlet</servlet-name>
    	<url-pattern>/addNewTeaching</url-pattern>
	</servlet-mapping> 
	
  	<servlet-mapping>
  		<servlet-name>ShowChargeTeacherServlet</servlet-name>
  		<url-pattern>/ShowChargeTeacherServlet</url-pattern>
  	</servlet-mapping>  
</web-app>



  
